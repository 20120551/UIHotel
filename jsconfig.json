{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "src/",
        "paths": {
            "@components/*": [
                "components/*"
            ],
            "@hooks/*": [
                "hooks/*"
            ],
            "@constant/*": [
                "constant/*"
            ],
            "@lib/*": [
                "lib/*"
            ],
            "@store/*": [
                "store/*"
            ],
            "@config/*": [
                "config/*"
            ],
            "@pages/*": [
                "pages/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}